source 'https://github.com/CocoaPods/Specs.git'
#platform :ios, '10.0'
use_frameworks!

target "MyDoors" do
    platform :ios, '10.0'
    pod 'Socket.IO-Client-Swift', :git => 'https://github.com/socketio/socket.io-client-swift.git'
    #pod 'Socket.IO-Client-Swift', :git => 'https://github.com/ricardopereira/SocketIO-Kit.git', :branch => 'Swift2'
    pod 'QRCodeReader'
    pod 'ReachabilitySwift', '~> 3'
end

target 'MyDoorsWatch Extension' do
    platform :watchos, '3.0'
    #pod 'Socket.IO-Client-Swift', :git => 'https://github.com/socketio/socket.io-client-swift.git'
    pod 'Alamofire'
end
